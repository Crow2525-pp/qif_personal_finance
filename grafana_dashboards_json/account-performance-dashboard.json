      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  DATE_TRUNC('month', t.date) as time,\n  CASE WHEN COUNT(CASE WHEN t.amount > 0 THEN 1 END) > 0 THEN 3410.00 ELSE 0 END as monthly_payment,\n  SUM(CASE WHEN t.amount < 0 THEN ABS(t.amount) ELSE 0 END) as total_charges_added,\n  CASE WHEN COUNT(CASE WHEN t.amount > 0 THEN 1 END) > 0 THEN 3410.00 ELSE 0 END - SUM(CASE WHEN t.amount < 0 THEN ABS(t.amount) ELSE 0 END) as net_principal_reduction\nFROM reporting.reporting__fact_transactions t \nJOIN reporting.dim_account a ON t.account_foreign_key = a.origin_key \nWHERE a.account_name = 'homeloan' \n  AND a.bank_name = 'bendigo'\n  AND DATE_TRUNC('month', t.date) < DATE_TRUNC('month', CURRENT_DATE)\nGROUP BY DATE_TRUNC('month', t.date) \nORDER BY time;",
          "refId": "A"
        }
      ],
      "title": "Mortgage Payment vs Interest & Principal Reduction",
