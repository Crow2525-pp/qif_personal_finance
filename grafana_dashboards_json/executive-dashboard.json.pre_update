          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH latest_data AS (\n  SELECT *\n  FROM reporting.rpt_executive_dashboard \n  ORDER BY dashboard_year DESC, dashboard_month_num DESC \n  LIMIT 1\n)\nSELECT\n  COALESCE((SELECT overall_financial_health_score FROM latest_data), 0)::double precision AS \"Financial Health\",\n  COALESCE((SELECT CASE \n             WHEN monthly_savings_rate_percent >= 0.30 THEN 100\n             WHEN monthly_savings_rate_percent >= 0.20 THEN 80\n             WHEN monthly_savings_rate_percent >= 0.15 THEN 60\n             WHEN monthly_savings_rate_percent >= 0.10 THEN 40\n             ELSE 20\n           END FROM latest_data), 0)::double precision AS \"Savings Performance\",\n  COALESCE((SELECT CASE net_worth_benchmark\n             WHEN 'High Net Worth' THEN 100\n             WHEN 'Building Wealth' THEN 80\n             WHEN 'Positive Net Worth' THEN 60\n             WHEN 'Rebuilding Phase' THEN 40\n             ELSE 20\n           END FROM latest_data), 0)::double precision AS \"Net Worth Progress\";",
          "refId": "A"
        }
