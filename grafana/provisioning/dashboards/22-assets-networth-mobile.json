      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH current_month AS (\n  SELECT\n    account_name,\n    account_type,\n    end_of_month_balance,\n    COALESCE(mom_balance_change, 0) AS mom_balance_change,\n    (end_of_month_balance - COALESCE(mom_balance_change, 0)) AS previous_balance\n  FROM reporting.rpt_account_performance\n  WHERE budget_year_month = (SELECT MAX(budget_year_month) FROM reporting.rpt_account_performance)\n), normalized AS (\n  SELECT\n    CASE\n      WHEN account_name = 'BillsBillsBills' THEN 'Bills'\n      WHEN account_name ILIKE '%homeloan%' THEN 'Home Loan'\n      WHEN account_name ILIKE '%offset%' THEN 'Offset Account'\n      WHEN account_name ILIKE '%credit%' THEN 'Credit Card'\n      ELSE account_name\n    END AS account_label,\n    CASE\n      WHEN account_type ILIKE '%liab%' OR end_of_month_balance < 0 THEN 'Liability'\n      ELSE 'Asset'\n    END AS account_class,\n    CASE\n      WHEN account_type ILIKE '%liab%' OR end_of_month_balance < 0 THEN ABS(end_of_month_balance)\n      ELSE end_of_month_balance\n    END AS current_balance_norm,\n    CASE\n      WHEN account_type ILIKE '%liab%' OR previous_balance < 0 THEN ABS(previous_balance)\n      ELSE previous_balance\n    END AS previous_balance_norm\n  FROM current_month\n)\nSELECT\n  account_label AS \"Account\",\n  account_class AS \"Type\",\n  ROUND(current_balance_norm, 2) AS \"Current Balance\",\n  ROUND(previous_balance_norm, 2) AS \"Previous Balance\",\n  ROUND(\n    CASE\n      WHEN account_class = 'Liability' THEN previous_balance_norm - current_balance_norm\n      ELSE current_balance_norm - previous_balance_norm\n    END,\n    2\n  ) AS \"Change (Positive = Improvement)\"\nFROM normalized\nORDER BY ABS(current_balance_norm) DESC;",
          "refId": "A"
        }
      ],
      "title": "Account Balances (Positive Change = Improvement)",
      "type": "table"
    },
