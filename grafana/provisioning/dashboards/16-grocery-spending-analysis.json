          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH baseline_budget AS (\n  SELECT COALESCE(\n    ROUND(AVG(total_grocery_spend), 2),\n    0\n  ) AS monthly_budget_target\n  FROM reporting.viz_grocery_monthly_summary\n  WHERE year_month >= DATE_TRUNC('month', NOW()) - INTERVAL '12 months'\n    AND year_month < DATE_TRUNC('month', NOW())\n), latest_month AS (\n  SELECT total_grocery_spend\n  FROM reporting.viz_grocery_monthly_summary\n  ORDER BY year_month DESC\n  LIMIT 1\n)\nSELECT\n  ROUND(b.monthly_budget_target - l.total_grocery_spend, 2) AS \"Budget Variance\",\n  ROUND(CASE\n    WHEN b.monthly_budget_target > 0 THEN (l.total_grocery_spend / b.monthly_budget_target) * 100\n    ELSE 0\n  END, 1) AS \"% of Budget\"\nFROM baseline_budget b\nCROSS JOIN latest_month l",
          "refId": "A"
        }
      ],
      "title": "Monthly Budget Target & Variance",
      "type": "stat"
    },
