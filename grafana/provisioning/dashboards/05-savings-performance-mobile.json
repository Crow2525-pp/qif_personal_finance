          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n    'Total Expenses' as \"Performance Metric\",\n    total_expenses as \"Current Month\",\n    (prev_month_income - prev_month_total_savings) as \"Previous Month\",\n    CASE \n        WHEN (prev_month_income - prev_month_total_savings) > 0 THEN \n            (total_expenses - (prev_month_income - prev_month_total_savings)) / (prev_month_income - prev_month_total_savings)\n        ELSE NULL\n    END as \"Change %\"\nFROM reporting.rpt_savings_analysis\nWHERE budget_year_month = (SELECT MAX(budget_year_month) FROM reporting.rpt_savings_analysis WHERE budget_year_month < TO_CHAR($__timeTo()::date, 'YYYY-MM'))\nUNION ALL\nSELECT \n    'Savings Rate',\n    total_savings_rate_percent,\n    COALESCE(prev_month_savings_rate, 0),\n    CASE \n        WHEN prev_month_savings_rate != 0 THEN \n            mom_savings_rate_change\n        ELSE NULL\n    END\nFROM reporting.rpt_savings_analysis\nWHERE budget_year_month = (SELECT MAX(budget_year_month) FROM reporting.rpt_savings_analysis WHERE budget_year_month < TO_CHAR($__timeTo()::date, 'YYYY-MM'));",
          "refId": "A"
        }
      ],
