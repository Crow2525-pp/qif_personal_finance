          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT \n    TO_TIMESTAMP(budget_year_month || '-01', 'YYYY-MM-DD')::date as time,\n    net_worth as \"Net Worth\",\n    total_assets as \"Assets\",\n    -total_liabilities as \"Liabilities\"\nFROM reporting.rpt_household_net_worth\nWHERE budget_year_month >= TO_CHAR(CURRENT_DATE - INTERVAL '24 months', 'YYYY-MM')\nORDER BY budget_year_month;",
