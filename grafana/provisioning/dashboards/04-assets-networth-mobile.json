          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH current_month AS (\n    SELECT \n        account_name,\n        account_type,\n        end_of_month_balance,\n        mom_balance_change\n    FROM reporting.rpt_account_performance\n    WHERE budget_year_month = (SELECT MAX(budget_year_month) FROM reporting.rpt_account_performance)\n)\nSELECT \n    account_name as \"Account\",\n    account_type as \"Type\",\n    end_of_month_balance as \"Current Balance\",\n    (end_of_month_balance - COALESCE(mom_balance_change, 0)) as \"Previous Balance\",\n    COALESCE(mom_balance_change, 0) as \"Change\"\nFROM current_month\nORDER BY ABS(end_of_month_balance) DESC;",
          "refId": "A"
        }
      ],
      "title": "Account Balances with Month-over-Month Changes",
      "type": "table"
    },
