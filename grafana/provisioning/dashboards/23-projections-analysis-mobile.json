      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH current_year_data AS (\n    SELECT \n        TO_TIMESTAMP(budget_year_month || '-01', 'YYYY-MM-DD')::date as time,\n        total_savings_rate_percent as \"Current Year\"\n    FROM reporting.rpt_savings_analysis\n    WHERE SUBSTRING(budget_year_month, 1, 4) = TO_CHAR(CURRENT_DATE, 'YYYY')\n),\nprevious_year_data AS (\n    SELECT \n        TO_TIMESTAMP((TO_CHAR(TO_DATE(budget_year_month, 'YYYY-MM') + INTERVAL '1 year', 'YYYY-MM') || '-01'), 'YYYY-MM-DD')::date as time,\n        total_savings_rate_percent as \"Previous Year\"\n    FROM reporting.rpt_savings_analysis\n    WHERE SUBSTRING(budget_year_month, 1, 4) = TO_CHAR(CURRENT_DATE - INTERVAL '1 year', 'YYYY')\n)\nSELECT \n    COALESCE(cy.time, py.time) as time,\n    cy.\"Current Year\",\n    py.\"Previous Year\"\nFROM current_year_data cy\nFULL OUTER JOIN previous_year_data py ON cy.time = py.time\nORDER BY time;",
          "refId": "A"
        }
      ],
      "title": "Year-over-Year Savings Rate Comparison",
      "type": "timeseries"
    },
