      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH latest_data AS (\n  SELECT *\n  FROM reporting.rpt_executive_dashboard \n  ORDER BY dashboard_year DESC, dashboard_month_num DESC \n  LIMIT 1\n)\nSELECT \n  'Expense-to-Income Ratio' as \"Efficiency Metric\",\n  ROUND(expense_to_income_ratio_pct, 1) as \"Current Value\",\n  '%' as \"Unit\",\n  CASE \n    WHEN expense_to_income_ratio_pct <= 70 THEN 'Excellent'\n    WHEN expense_to_income_ratio_pct <= 80 THEN 'Good'\n    WHEN expense_to_income_ratio_pct <= 90 THEN 'Fair'\n    WHEN expense_to_income_ratio_pct <= 100 THEN 'Poor'\n    ELSE 'Critical'\n  END as \"Performance Rating\",\n  'Lower is better. Target <80%' as \"Notes\"\nFROM latest_data\nUNION ALL\nSELECT \n  'Monthly Savings Rate',\n  ROUND(monthly_savings_rate_percent_pct, 1),\n  '%',\n  CASE \n    WHEN monthly_savings_rate_percent_pct >= 20 THEN 'Excellent'\n    WHEN monthly_savings_rate_percent_pct >= 15 THEN 'Good'\n    WHEN monthly_savings_rate_percent_pct >= 10 THEN 'Fair'\n    WHEN monthly_savings_rate_percent_pct >= 5 THEN 'Poor'\n    ELSE 'Critical'\n  END,\n  'Higher is better. Target >15%'\nFROM latest_data\nUNION ALL\nSELECT \n  'Cash Flow Score',\n  cash_flow_score,\n  'pts',\n  CASE \n    WHEN cash_flow_score >= 80 THEN 'Excellent'\n    WHEN cash_flow_score >= 70 THEN 'Good'\n    WHEN cash_flow_score >= 60 THEN 'Fair'\n    WHEN cash_flow_score >= 50 THEN 'Poor'\n    ELSE 'Critical'\n  END,\n  'Composite efficiency score'\nFROM latest_data",
          "refId": "A"
        }
      ],
      "title": "Expense Performance Metrics Summary",
      "type": "table"
    }
