          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH expense_trends AS (\n  SELECT \n    to_date(budget_year_month || '-01', 'YYYY-MM-DD') AS time,\n    expense_ratio_percent AS \"Expense Ratio (%)\",\n    0.80 AS \"Target (80%)\"\n  FROM reporting.rpt_monthly_budget_summary \n  WHERE to_date(budget_year_month || '-01', 'YYYY-MM-DD') < date_trunc('month', CURRENT_DATE)\n    AND to_date(budget_year_month || '-01', 'YYYY-MM-DD') >= date_trunc('month', CURRENT_DATE) - INTERVAL '12 months'\n  ORDER BY time\n)\nSELECT * FROM expense_trends",
          "refId": "A"
        }
      ],
      "title": "Expense Ratio Trend (12 Months)",
      "type": "timeseries"
