      "targets": [
        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH latest_mortgage AS (\n  SELECT\n    outstanding_debt_balance,\n    avg_3month_principal,\n    estimated_interest_rate_pct\n  FROM reporting.rpt_debt_reduction_tracking\n  WHERE is_mortgage = TRUE\n  ORDER BY period_month DESC\n  LIMIT 1\n),\nsensitivity_scenarios AS (\n  SELECT 0 AS extra_payment,\n         CASE WHEN lm.avg_3month_principal > 0\n              THEN CEIL(lm.outstanding_debt_balance / lm.avg_3month_principal) / 12\n              ELSE NULL END AS years_to_payoff\n  FROM latest_mortgage lm\n  UNION ALL\n  SELECT 100,\n         CASE WHEN (lm.avg_3month_principal + 100) > 0\n              THEN CEIL(lm.outstanding_debt_balance / (lm.avg_3month_principal + 100)) / 12\n              ELSE NULL END\n  FROM latest_mortgage lm\n  UNION ALL\n  SELECT 250,\n         CASE WHEN (lm.avg_3month_principal + 250) > 0\n              THEN CEIL(lm.outstanding_debt_balance / (lm.avg_3month_principal + 250)) / 12\n              ELSE NULL END\n  FROM latest_mortgage lm\n  UNION ALL\n  SELECT 500,\n         CASE WHEN (lm.avg_3month_principal + 500) > 0\n              THEN CEIL(lm.outstanding_debt_balance / (lm.avg_3month_principal + 500)) / 12\n              ELSE NULL END\n  FROM latest_mortgage lm\n  UNION ALL\n  SELECT 1000,\n         CASE WHEN (lm.avg_3month_principal + 1000) > 0\n              THEN CEIL(lm.outstanding_debt_balance / (lm.avg_3month_principal + 1000)) / 12\n              ELSE NULL END\n  FROM latest_mortgage lm\n)\nSELECT extra_payment,\n       COALESCE(ROUND(years_to_payoff::numeric, 1), 0) AS \"Years to Payoff\"\nFROM sensitivity_scenarios\nUNION ALL\nSELECT 0 AS extra_payment, 0::numeric AS \"Years to Payoff\"\nWHERE NOT EXISTS (SELECT 1 FROM sensitivity_scenarios)\nORDER BY extra_payment;",
          "refId": "A",
