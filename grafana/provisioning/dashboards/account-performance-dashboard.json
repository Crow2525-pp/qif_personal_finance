      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  DATE_TRUNC('month', ft.transaction_date)::timestamptz AS time,\n  SUM(CASE WHEN ft.transaction_direction = 'Debit' THEN ft.transaction_amount ELSE 0 END) AS monthly_payment,\n  SUM(CASE WHEN ft.transaction_direction = 'Credit' THEN ABS(ft.transaction_amount) ELSE 0 END) AS total_charges_added,\n  SUM(CASE WHEN ft.transaction_direction = 'Debit' THEN ft.transaction_amount ELSE 0 END) -\n    SUM(CASE WHEN ft.transaction_direction = 'Credit' THEN ABS(ft.transaction_amount) ELSE 0 END) AS net_principal_reduction\nFROM reporting.fct_transactions ft\nJOIN reporting.dim_accounts da ON ft.account_key = da.account_key \nWHERE LOWER(da.account_name) = 'homeloan'\n  AND $__timeFilter(ft.transaction_date)\n  AND DATE_TRUNC('month', ft.transaction_date) < DATE_TRUNC('month', CURRENT_DATE)\nGROUP BY DATE_TRUNC('month', ft.transaction_date) \nORDER BY time;",
          "refId": "A"
        }
      ],
      "title": "Mortgage Payment vs Interest & Principal Reduction",
      "transformations": [
