      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH monthly_expenses AS (\n  SELECT \n    budget_year_month,\n    total_expenses\n  FROM reporting.rpt_monthly_budget_summary\n  WHERE budget_year_month >= TO_CHAR(date_trunc('month', $__timeTo()::timestamptz) - INTERVAL '2 months', 'YYYY-MM')\n    AND budget_year_month < TO_CHAR(date_trunc('month', $__timeTo()::timestamptz), 'YYYY-MM')\n  ORDER BY budget_year_month DESC\n  LIMIT 2\n),\nexpense_comparison AS (\n  SELECT \n    (ARRAY_AGG(total_expenses ORDER BY budget_year_month DESC))[1] as current_month,\n    (ARRAY_AGG(total_expenses ORDER BY budget_year_month DESC))[2] as previous_month,\n    (ARRAY_AGG(budget_year_month ORDER BY budget_year_month DESC))[1] as current_period,\n    (ARRAY_AGG(budget_year_month ORDER BY budget_year_month DESC))[2] as previous_period\n  FROM monthly_expenses\n)\nSELECT \n  'Total Monthly Expenses' as \"Category\",\n  current_month as \"Current Month ($)\",\n  previous_month as \"Previous Month ($)\",\n  (current_month - previous_month) as \"Change ($)\",\n  CASE \n    WHEN previous_month > 0 THEN ((current_month - previous_month) / previous_month) * 100\n    ELSE 0\n  END as \"Change (%)\",\n  current_period as \"Current Period\",\n  previous_period as \"Previous Period\"\nFROM expense_comparison",
          "refId": "A"
        }
      ],
      "title": "Monthly Expense Change Analysis",
      "type": "table"
