      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH latest_data AS (\n  SELECT \n    COALESCE(expense_ratio_percent, 0) * 100 AS expense_ratio_pct,\n    COALESCE(savings_rate_percent, 0) * 100 AS savings_rate_pct,\n    CASE\n      WHEN COALESCE(savings_rate_percent, 0) >= 0.20 THEN 90\n      WHEN COALESCE(savings_rate_percent, 0) >= 0.15 THEN 80\n      WHEN COALESCE(savings_rate_percent, 0) >= 0.10 THEN 70\n      WHEN COALESCE(savings_rate_percent, 0) >= 0.05 THEN 60\n      ELSE 45\n    END AS cash_flow_score\n  FROM reporting.rpt_monthly_budget_summary\n  WHERE budget_year_month < TO_CHAR(CURRENT_DATE, 'YYYY-MM')\n  ORDER BY budget_year DESC, budget_month DESC\n  LIMIT 1\n)\nSELECT \n  'Expense-to-Income Ratio' as \"Efficiency Metric\",\n  ROUND(expense_ratio_pct, 1) as \"Current Value\",\n  '%' as \"Unit\",\n  CASE \n    WHEN expense_ratio_pct <= 70 THEN 'Excellent'\n    WHEN expense_ratio_pct <= 80 THEN 'Good'\n    WHEN expense_ratio_pct <= 90 THEN 'Fair'\n    WHEN expense_ratio_pct <= 100 THEN 'Poor'\n    ELSE 'Critical'\n  END as \"Performance Rating\",\n  'Lower is better. Target <80%' as \"Notes\"\nFROM latest_data\nUNION ALL\nSELECT \n  'Monthly Savings Rate',\n  ROUND(savings_rate_pct, 1),\n  '%',\n  CASE \n    WHEN savings_rate_pct >= 20 THEN 'Excellent'\n    WHEN savings_rate_pct >= 15 THEN 'Good'\n    WHEN savings_rate_pct >= 10 THEN 'Fair'\n    WHEN savings_rate_pct >= 5 THEN 'Poor'\n    ELSE 'Critical'\n  END,\n  'Higher is better. Target >15%'\nFROM latest_data\nUNION ALL\nSELECT \n  'Cash Flow Score',\n  cash_flow_score,\n  'pts',\n  CASE \n    WHEN cash_flow_score >= 80 THEN 'Excellent'\n    WHEN cash_flow_score >= 70 THEN 'Good'\n    WHEN cash_flow_score >= 60 THEN 'Fair'\n    WHEN cash_flow_score >= 50 THEN 'Poor'\n    ELSE 'Critical'\n  END,\n  'Derived from latest monthly savings rate'\nFROM latest_data",
          "refId": "A"
        }
      ],
      "title": "Expense Performance Metrics Summary",
      "type": "table"
