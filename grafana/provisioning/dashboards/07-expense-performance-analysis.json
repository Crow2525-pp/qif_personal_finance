          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH latest_month AS (\n  SELECT MAX(budget_year_month) as latest_month\n  FROM reporting.rpt_monthly_budget_summary\n  WHERE budget_year_month < TO_CHAR(CURRENT_DATE, 'YYYY-MM')\n),\ncategory_expenses AS (\n  SELECT \n    ct.level_1_category as \"Category\",\n    ct.monthly_spending as \"Monthly Amount ($)\",\n    ct.percent_of_total_monthly_spending as \"% of Total Expenses\",\n    ct.spending_trend_category as \"Trend\"\n  FROM reporting.rpt_category_spending_trends ct\n  CROSS JOIN latest_month lm\n  WHERE ct.budget_year_month = lm.latest_month\n    AND ct.monthly_spending > 0\n  ORDER BY ct.monthly_spending DESC\n  LIMIT 10\n)\nSELECT * FROM category_expenses",
          "refId": "A"
        }
