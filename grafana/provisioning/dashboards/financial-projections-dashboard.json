      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH historical AS (\n  SELECT \n    DATE_TRUNC('month', transaction_date) as time,\n    SUM(CASE WHEN transaction_amount > 0 THEN transaction_amount ELSE 0 END) as actual_income,\n    SUM(CASE WHEN transaction_amount < 0 THEN ABS(transaction_amount) ELSE 0 END) as actual_expenses,\n    SUM(transaction_amount) as actual_net_flow,\n    'Historical' as data_type\n  FROM reporting.fct_transactions \n  WHERE transaction_date >= '2022-01-01' AND transaction_date < DATE_TRUNC('month', CURRENT_DATE)\n  GROUP BY DATE_TRUNC('month', transaction_date)\n),\nprojected AS (\n  SELECT \n    projection_date as time,\n    projected_monthly_income as actual_income,\n    projected_monthly_expenses as actual_expenses,\n    projected_net_flow as actual_net_flow,\n    CONCAT('Projected (', scenario, ')') as data_type\n  FROM reporting.projection_data\n)\nSELECT \n  time,\n  data_type,\n  actual_net_flow as value\nFROM historical\nUNION ALL\nSELECT \n  time,\n  data_type,\n  actual_net_flow as value\nFROM projected\nORDER BY time, data_type",
          "refId": "A"
        }
      ],
      "title": "YoY Comparison: Historical vs Projected Net Flow",
      "type": "timeseries"
    },
