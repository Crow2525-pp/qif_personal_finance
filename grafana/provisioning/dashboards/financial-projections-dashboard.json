      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH opportunity_cost AS (\n  SELECT \n    ft.projected_monthly_income - pt.projected_monthly_income as monthly_opportunity_cost,\n    ft.cumulative_net_flow - pt.cumulative_net_flow as cumulative_opportunity_cost\n  FROM reporting.projection_data ft\n  JOIN reporting.projection_data pt ON ft.projection_month = pt.projection_month\n  WHERE ft.scenario = 'full_time' AND pt.scenario = 'part_time'\n  AND ft.projection_month = 12\n)\nSELECT \n  'Annual Income Opportunity Cost' as metric,\n  monthly_opportunity_cost * 12 as value\nFROM opportunity_cost\nUNION ALL\nSELECT \n  'Cumulative Opportunity Cost (12 months)' as metric,\n  cumulative_opportunity_cost as value\nFROM opportunity_cost",
          "refId": "A"
        }
      ],
      "title": "Part-time Work Opportunity Cost",
      "type": "piechart"
