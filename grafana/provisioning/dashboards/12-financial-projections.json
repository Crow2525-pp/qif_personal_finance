      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH horizon AS (\n  SELECT COALESCE(MIN(max_projection_month), 12) AS projection_horizon_month\n  FROM (\n    SELECT scenario, MAX(projection_month) AS max_projection_month\n    FROM reporting.projection_data\n    WHERE scenario IN ('full_time', 'part_time')\n    GROUP BY scenario\n  ) s\n),\nopportunity_cost AS (\n  SELECT \n    ft.projected_monthly_income - pt.projected_monthly_income as monthly_opportunity_cost,\n    ft.cumulative_net_flow - pt.cumulative_net_flow as cumulative_opportunity_cost,\n    h.projection_horizon_month\n  FROM reporting.projection_data ft\n  JOIN reporting.projection_data pt\n    ON ft.projection_month = pt.projection_month\n   AND ft.scenario = 'full_time'\n   AND pt.scenario = 'part_time'\n  CROSS JOIN horizon h\n  WHERE ft.projection_month = h.projection_horizon_month\n)\nSELECT \n  'Annual Income Opportunity Cost' as metric,\n  monthly_opportunity_cost * 12 as value\nFROM opportunity_cost\nUNION ALL\nSELECT \n  CONCAT('Cumulative Opportunity Cost (', projection_horizon_month::text, ' months)') as metric,\n  cumulative_opportunity_cost as value\nFROM opportunity_cost\nUNION ALL\nSELECT 'Annual Income Opportunity Cost', 0\nWHERE NOT EXISTS (SELECT 1 FROM opportunity_cost)\nUNION ALL\nSELECT 'Cumulative Opportunity Cost (No data)', 0\nWHERE NOT EXISTS (SELECT 1 FROM opportunity_cost)",
          "refId": "A"
        }
      ],
      "title": "Part-time Work Opportunity Cost",
      "type": "piechart"
    },
