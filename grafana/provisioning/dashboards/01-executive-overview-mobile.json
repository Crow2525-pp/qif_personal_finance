          "rawSql": "SELECT \n    'Net Flow' as \"Metric\",\n    monthly_net_cash_flow as \"Value\"\nFROM reporting.rpt_executive_dashboard\nWHERE dashboard_month = (SELECT MAX(dashboard_month) FROM reporting.rpt_executive_dashboard)\nUNION ALL\nSELECT \n    'Income',\n    monthly_income\nFROM reporting.rpt_executive_dashboard\nWHERE dashboard_month = (SELECT MAX(dashboard_month) FROM reporting.rpt_executive_dashboard)\nUNION ALL\nSELECT \n    'Expenses',\n    -monthly_expenses\nFROM reporting.rpt_executive_dashboard\nWHERE dashboard_month = (SELECT MAX(dashboard_month) FROM reporting.rpt_executive_dashboard)\nUNION ALL\nSELECT\n    'Savings Rate',\n    monthly_savings_rate_percent\nFROM reporting.rpt_executive_dashboard\nWHERE dashboard_month = (SELECT MAX(dashboard_month) FROM reporting.rpt_executive_dashboard);",
          "refId": "A"
        }
