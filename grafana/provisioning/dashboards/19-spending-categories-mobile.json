          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH aldi_monthly AS (\n    SELECT \n        DATE_TRUNC('month', transaction_date) as month,\n        ABS(SUM(transaction_amount)) as aldi_spend\n    FROM reporting.fct_transactions \n    WHERE transaction_memo ILIKE '%ALDI STORES%' \n        AND transaction_amount < 0 \n        AND transaction_date >= CURRENT_DATE - INTERVAL '12 months'\n    GROUP BY DATE_TRUNC('month', transaction_date)\n),\ngrocery_with_aldi AS (\n    SELECT \n        gms.coles_spend,\n        gms.woolworths_spend, \n        gms.gaskos_spend,\n        COALESCE(am.aldi_spend, 0) as aldi_spend,\n        gms.total_grocery_spend + COALESCE(am.aldi_spend, 0) as total_with_aldi\n    FROM reporting.viz_grocery_monthly_summary gms\n    LEFT JOIN aldi_monthly am ON gms.year_month = am.month\n    WHERE gms.year_month >= NOW() - INTERVAL '12 months'\n)\nSELECT \n    ROUND(AVG(coles_spend), 2) as \"Coles 12m Avg\", \n    ROUND(AVG(woolworths_spend), 2) as \"Woolworths 12m Avg\", \n    ROUND(AVG(gaskos_spend), 2) as \"Gaskos 12m Avg\",\n    ROUND(AVG(aldi_spend), 2) as \"ALDI 12m Avg\",\n    ROUND(AVG(total_with_aldi), 2) as \"Total 12m Avg\" \nFROM grocery_with_aldi",
          "refId": "A"
        }
      ],
      "title": "12-Month Average Grocery Spending by Retailer",
      "type": "stat"
